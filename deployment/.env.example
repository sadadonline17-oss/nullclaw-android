# NullClaw Backend Configuration
# Copy this file to .env and fill in your values

# API Configuration
NULLCLAW_API_KEY=sk-or-your-api-key-here
NULLCLAW_PROVIDER=openrouter
NULLCLAW_MODEL=anthropic/claude-sonnet-4

# Alternative providers (uncomment to use)
# ANTHROPIC_API_KEY=sk-ant-your-key
# OPENAI_API_KEY=sk-your-key

# Gateway Configuration
NULLCLAW_GATEWAY_PORT=3000
NULLCLAW_REQUIRE_PAIRING=true

# Tunnel Configuration (optional)
# CLOUDFLARE_TUNNEL_TOKEN=your-cloudflare-tunnel-token
# NGROK_AUTHTOKEN=your-ngrok-auth-token

# Security
NULLCLAW_ENCRYPT_SECRETS=true